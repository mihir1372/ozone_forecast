   "source": [
    "# Convert 'Date' to datetime and set it as the index\n",
    "data['Date'] = pd.to_datetime(data['Date'])\n",
    "data.set_index('Date', inplace=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "8e9e9dce",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Column of interest\n",
    "column = 'Daily Max 8-hour Ozone Concentration'\n",
    "\n",
    "if column in data.columns:\n",
    "    data[column] = data[column].round(3)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "9dee8c93",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Step 1: Remove extreme gaps (detect large stretches of NaN or near-zero values)\n",
    "threshold = 0.001  # Threshold to identify near-zero or invalid values\n",
    "data[column] = data[column].where(data[column] > threshold, None)  # Set near-zero values to NaN"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "61e70f76",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Step 2: Interpolate missing values using time-weighted interpolation\n",
    "data[column] = data[column].interpolate(method='time')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "d4a1f423",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Step 3: Apply seasonal averages to improve gaps further (if long gaps exist)\n",
    "if data[column].isnull().sum() > 0:\n",
    "    # Calculate seasonal (monthly) averages and fill remaining gaps\n",
    "    monthly_avg = data[column].groupby(data.index.month).transform('mean')\n",
    "    data[column].fillna(monthly_avg, inplace=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "fbf6a992",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Step 4: Drop any remaining rows with missing or invalid data\n",
    "data.dropna(subset=[column], inplace=True)"
   ]
  },
